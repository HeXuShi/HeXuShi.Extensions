<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/HeXuShi/HeXuShi.Extensions</PackageProjectUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <SignAssembly>True</SignAssembly>
    <Version>6.0.0</Version>
    <RepositoryUrl>https://github.com/HeXuShi/HeXuShi.Extensions</RepositoryUrl>
    <RepositoryType>open source</RepositoryType>
    <PackageTags>web http cn china Middleware</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>When you use China mainland ip access, change xx.com or xx.anything to xx.cn,help you jump to cn(china) domain suffix.</Description>
    <Copyright>Copyright 2022</Copyright>
    <RootNamespace>HeXuShi.Extensions</RootNamespace>
    <PackageReleaseNotes>add complex interfaces</PackageReleaseNotes>
    <PackageIcon>35529176.jpg</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt" Link="LICENSE.txt">
      <PackagePath></PackagePath>
      <Pack>True</Pack>
    </None>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="images\35529176.jpg" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HeXuShi.Extensions.IsChinaIpAddress" Version="3.0.0" />
  </ItemGroup>

</Project>
